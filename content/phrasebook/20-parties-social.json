[
  {"id": "parties-001", "english": "Would you like to come to my party?", "georgian": "გინდა ჩემს წვეულებაზე მობრძანება?", "literal": "Want you to my party coming?", "context": "Inviting someone to a social gathering", "level": "B1", "category": "parties-social"},
  {"id": "parties-002", "english": "I'm having a party this weekend", "georgian": "ამ უიკენდს წვეულებას ვმართავ", "literal": "This weekend party I'm organizing", "context": "Announcing your social event", "level": "B1", "category": "parties-social"},
  {"id": "parties-003", "english": "Thanks for inviting me!", "georgian": "მადლობა მოწვევისთვის!", "literal": "Thank you for invitation!", "context": "Expressing gratitude for invitation", "level": "B1", "category": "parties-social"},
  {"id": "parties-004", "english": "I'd love to come", "georgian": "სიამოვნებით მოვალ", "literal": "With pleasure I will come", "context": "Accepting a social invitation", "level": "B1", "category": "parties-social"},
  {"id": "parties-005", "english": "Sorry, I can't make it", "georgian": "ბოდიში, ვერ მოვალ", "literal": "Sorry, cannot I come", "context": "Politely declining invitation", "level": "B1", "category": "parties-social"},
  {"id": "parties-006", "english": "What should I bring?", "georgian": "რა უნდა მოვიტანო?", "literal": "What should I bring?", "context": "Asking about party contributions", "level": "B1", "category": "parties-social"},
  {"id": "parties-007", "english": "Should I bring anything?", "georgian": "რამე უნდა მოვიტანო?", "literal": "Something should I bring?", "context": "Offering to contribute to party", "level": "B1", "category": "parties-social"},
  {"id": "parties-008", "english": "What time does the party start?", "georgian": "რომელ საათზე იწყება წვეულება?", "literal": "At what hour starts party?", "context": "Asking about event timing", "level": "B1", "category": "parties-social"},
  {"id": "parties-009", "english": "Where is the party?", "georgian": "სად არის წვეულება?", "literal": "Where is party?", "context": "Getting location information", "level": "B1", "category": "parties-social"},
  {"id": "parties-010", "english": "Can I bring a friend?", "georgian": "შემიძლია მეგობრის მოყვანა?", "literal": "Can I friend bringing?", "context": "Asking about plus-ones", "level": "B1", "category": "parties-social"},
  {"id": "parties-011", "english": "Great party!", "georgian": "მშვენიერი წვეულება!", "literal": "Beautiful party!", "context": "Complimenting the host", "level": "B1", "category": "parties-social"},
  {"id": "parties-012", "english": "I'm having a great time", "georgian": "ძალიან კარგად ვიშხაწებ", "literal": "Very well I'm amusing myself", "context": "Expressing enjoyment at party", "level": "B1", "category": "parties-social"},
  {"id": "parties-013", "english": "Let me introduce you", "georgian": "მოდი გაგაცნოთ", "literal": "Come let me introduce you", "context": "Making introductions at social events", "level": "B1", "category": "parties-social"},
  {"id": "parties-014", "english": "Have we met before?", "georgian": "ადრე ხომ არ შევხვედრილვარ?", "literal": "Before haven't we met?", "context": "When you think you recognize someone", "level": "B1", "category": "parties-social"},
  {"id": "parties-015", "english": "Nice to meet you", "georgian": "სასიამოვნოა გაცნობა", "literal": "Pleasant is acquaintance", "context": "Standard greeting for new people", "level": "B1", "category": "parties-social"},
  {"id": "parties-016", "english": "How do you know the host?", "georgian": "როგორ იცნობ მასპინძელს?", "literal": "How you know host?", "context": "Small talk at parties", "level": "B1", "category": "parties-social"},
  {"id": "parties-017", "english": "What would you like to drink?", "georgian": "რა გინდა დალიო?", "literal": "What want you to drink?", "context": "Offering beverages to guests", "level": "B1", "category": "parties-social"},
  {"id": "parties-018", "english": "I'll have a beer", "georgian": "ლუდს დავლევ", "literal": "Beer I will drink", "context": "Ordering alcoholic drink", "level": "B1", "category": "parties-social"},
  {"id": "parties-019", "english": "Just water, please", "georgian": "მხოლოდ წყალი, მადლობა", "literal": "Only water, thank you", "context": "Requesting non-alcoholic option", "level": "B1", "category": "parties-social"},
  {"id": "parties-020", "english": "Cheers!", "georgian": "გაუმარჯოს!", "literal": "Victory to it!", "context": "Traditional Georgian toast", "level": "B1", "category": "parties-social"},
  {"id": "parties-021", "english": "Let's make a toast", "georgian": "მოდი სუფრა ავიღოთ", "literal": "Come let's take supra", "context": "Suggesting a Georgian-style toast", "level": "B1", "category": "parties-social"},
  {"id": "parties-022", "english": "Would you like to dance?", "georgian": "გინდა ვიცეკვოთ?", "literal": "Want you we dance?", "context": "Inviting someone to dance", "level": "B1", "category": "parties-social"},
  {"id": "parties-023", "english": "I don't know how to dance", "georgian": "ვერ ვცეკვავ", "literal": "Cannot I dance", "context": "Declining dance invitation", "level": "B1", "category": "parties-social"},
  {"id": "parties-024", "english": "The music is great!", "georgian": "მუსიკა მშვენიერია!", "literal": "Music beautiful is!", "context": "Complimenting party entertainment", "level": "B1", "category": "parties-social"},
  {"id": "parties-025", "english": "Can you turn up the music?", "georgian": "შეგიძლია მუსიკის ხმის აწევა?", "literal": "Can you music volume raising?", "context": "Requesting louder music", "level": "B1", "category": "parties-social"},
  {"id": "parties-026", "english": "The music is too loud", "georgian": "მუსიკა ძალიან ხმაურიანია", "literal": "Music very loud is", "context": "Complaining about noise level", "level": "B1", "category": "parties-social"},
  {"id": "parties-027", "english": "What time does the party end?", "georgian": "რომელ საათზე მთავრდება წვეულება?", "literal": "At what hour ends party?", "context": "Planning your evening", "level": "B1", "category": "parties-social"},
  {"id": "parties-028", "english": "I should probably head home", "georgian": "ალბათ უნდა წავიდე სახლში", "literal": "Probably should I go home", "context": "Signaling intention to leave", "level": "B1", "category": "parties-social"},
  {"id": "parties-029", "english": "Thanks for a wonderful evening", "georgian": "მადლობა ღამის გამო", "literal": "Thank you for night", "context": "Thanking host when leaving", "level": "B1", "category": "parties-social"},
  {"id": "parties-030", "english": "I had a great time", "georgian": "ძალიან კარგი დრო გავატარე", "literal": "Very good time I spent", "context": "Expressing satisfaction when leaving", "level": "B1", "category": "parties-social"},
  {"id": "parties-031", "english": "We should do this again", "georgian": "ასეთი რამ კიდევ უნდა გავაკეთოთ", "literal": "Such thing again should we do", "context": "Suggesting future gatherings", "level": "B1", "category": "parties-social"},
  {"id": "parties-032", "english": "Let's exchange numbers", "georgian": "ნომრები გავცვალოთ", "literal": "Numbers let's exchange", "context": "Wanting to stay in touch", "level": "B1", "category": "parties-social"},
  {"id": "parties-033", "english": "Are you on social media?", "georgian": "სოციალურ მედიაში ხარ?", "literal": "In social media are you?", "context": "Asking about online presence", "level": "B1", "category": "parties-social"},
  {"id": "parties-034", "english": "Can I add you on Instagram?", "georgian": "შემიძლია Instagram-ზე დაგამატო?", "literal": "Can I on Instagram add you?", "context": "Requesting social media connection", "level": "B1", "category": "parties-social"},
  {"id": "parties-035", "english": "Do you want to grab coffee sometime?", "georgian": "გინდა ერთხელ ყავა დავლიოთ?", "literal": "Want you once coffee we drink?", "context": "Suggesting future meetup", "level": "B1", "category": "parties-social"},
  {"id": "parties-036", "english": "I'm feeling a bit tipsy", "georgian": "ოდნავ მომიდუნდა", "literal": "Slightly it got to me", "context": "Admitting to being mildly intoxicated", "level": "B1", "category": "parties-social"},
  {"id": "parties-037", "english": "I think I've had enough to drink", "georgian": "ვფიქრობ საკმარისად დავლიე", "literal": "I think enough I drank", "context": "Recognizing drinking limits", "level": "B1", "category": "parties-social"},
  {"id": "parties-038", "english": "Can you call me a taxi?", "georgian": "შეგიძლია ტაქსი გამიძახო?", "literal": "Can you taxi call for me?", "context": "Requesting safe transport", "level": "B1", "category": "parties-social"},
  {"id": "parties-039", "english": "Is there somewhere I can sit down?", "georgian": "სადმე შემიძლია დავჯდე?", "literal": "Somewhere can I sit?", "context": "Looking for a place to rest", "level": "B1", "category": "parties-social"},
  {"id": "parties-040", "english": "Hope to see you again soon", "georgian": "იმედია მალე კიდევ გნახავ", "literal": "Hope is soon again I'll see you", "context": "Parting words at social events", "level": "B1", "category": "parties-social"}
