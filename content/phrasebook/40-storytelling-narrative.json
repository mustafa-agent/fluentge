[
  {"id": "storytelling-001", "english": "Once upon a time", "georgian": "იყო და არა იყო რა", "literal": "Was and wasn't what", "context": "Classic fairy tale opening", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-002", "english": "You won't believe what happened", "georgian": "ვერ წარმოიდგენ, რა მოხდა", "literal": "Can't imagine what happened", "context": "Building suspense before telling a story", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-003", "english": "So there I was, minding my own business", "georgian": "ასე ვიყავი, ჩემს საქმეს ვაკეთებდი", "literal": "So I was, my business I was doing", "context": "Casual story opening", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-004", "english": "Out of nowhere", "georgian": "არსაიდან", "literal": "From nowhere", "context": "Something unexpected happened", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-005", "english": "Long story short", "georgian": "მოკლედ რომ ვთქვა", "literal": "Shortly if I say", "context": "Summarizing a long story", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-006", "english": "And that's when things got crazy", "georgian": "და სწორედ მაშინ გაგიჟდა ყველაფერი", "literal": "And exactly then everything went crazy", "context": "Plot twist moment", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-007", "english": "I'm not making this up", "georgian": "არ ვტყუი, სერიოზულად", "literal": "Not I lie, seriously", "context": "Emphasizing story is true", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-008", "english": "Picture this", "georgian": "წარმოიდგინე", "literal": "Imagine", "context": "Setting a scene for the listener", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-009", "english": "It was a dark and stormy night", "georgian": "ბნელი და ქარიშხლიანი ღამე იყო", "literal": "Dark and stormy night was", "context": "Dramatic story opening (cliché)", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-010", "english": "And then the plot thickened", "georgian": "და მერე ამბავი უფრო გართულდა", "literal": "And then story more complicated became", "context": "Things got more complex", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-011", "english": "Wait, it gets better", "georgian": "დაიცა, ამაზე უკეთესი იქნება", "literal": "Wait, than this better will be", "context": "Building anticipation", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-012", "english": "To make a long story short", "georgian": "რომ მოგიყვე მოკლედ", "literal": "That I tell you shortly", "context": "Abbreviating a lengthy story", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-013", "english": "Meanwhile, back at the ranch", "georgian": "ამასობაში, სხვაგან", "literal": "Meanwhile, elsewhere", "context": "Switching to a parallel storyline", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-014", "english": "Little did I know", "georgian": "მაშინ ვერ წარმოვიდგენდი", "literal": "Then couldn't I imagine", "context": "Foreshadowing something", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-015", "english": "The next thing I knew", "georgian": "მომდევნო წამს", "literal": "Next second", "context": "Something happened suddenly", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-016", "english": "It all started when", "georgian": "ყველაფერი მაშინ დაიწყო, როცა", "literal": "Everything then began, when", "context": "Story origin", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-017", "english": "And they lived happily ever after", "georgian": "და ბედნიერად ცხოვრობდნენ", "literal": "And happily they lived", "context": "Classic fairy tale ending", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-018", "english": "Plot twist!", "georgian": "მოულოდნელი ტვისტი!", "literal": "Unexpected twist!", "context": "Announcing surprise in story", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-019", "english": "That's when I realized", "georgian": "სწორედ მაშინ მივხვდი", "literal": "Exactly then I understood", "context": "Moment of understanding", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-020", "english": "But here's the thing", "georgian": "მაგრამ აი, რა არის საქმე", "literal": "But here, what is the matter", "context": "Introducing key detail", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-021", "english": "It felt like time stood still", "georgian": "თითქოს დრო გაჩერდა", "literal": "As if time stopped", "context": "Dramatic pause in story", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-022", "english": "I couldn't believe my eyes", "georgian": "თვალებს ვერ ვუჯერებდი", "literal": "Eyes I couldn't believe", "context": "Shock or amazement", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-023", "english": "The room went silent", "georgian": "ოთახში სიჩუმე ჩამოვარდა", "literal": "In room silence fell", "context": "Dramatic moment", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-024", "english": "My heart was pounding", "georgian": "გული გამოსვლას მიპირებდა", "literal": "Heart coming out was intending", "context": "Describing fear or excitement", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-025", "english": "I was on the edge of my seat", "georgian": "სავარძლის კიდეზე ვიჯექი", "literal": "Chair's edge on I was sitting", "context": "Describing suspense", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-026", "english": "To cut to the chase", "georgian": "პირდაპირ საქმეზე რომ გადავიდე", "literal": "Directly to business if I go", "context": "Getting to the point", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-027", "english": "As luck would have it", "georgian": "როგორც იღბალმა ინება", "literal": "As luck wanted", "context": "Coincidence in a story", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-028", "english": "Against all odds", "georgian": "ყველა დაბრკოლების მიუხედავად", "literal": "All obstacles despite", "context": "Overcoming difficult situation", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-029", "english": "In the end, it all worked out", "georgian": "ბოლოს, ყველაფერი კარგად დასრულდა", "literal": "At end, everything well ended", "context": "Happy ending", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-030", "english": "I learned my lesson the hard way", "georgian": "გაკვეთილი რთულად ვისწავლე", "literal": "Lesson hard I learned", "context": "Story moral", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-031", "english": "If only I had known", "georgian": "ნეტავ ვიცოდე მაშინ", "literal": "I wish I knew then", "context": "Expressing regret about past", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-032", "english": "The rest is history", "georgian": "დანარჩენი ისტორიაა", "literal": "Rest history is", "context": "Everyone knows what happened after", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-033", "english": "It was too good to be true", "georgian": "ზედმეტად კარგი იყო, რომ ნამდვილი ყოფილიყო", "literal": "Too good it was, that real it would be", "context": "Something seemed perfect but wasn't", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-034", "english": "The tension was unbearable", "georgian": "დაძაბულობა აუტანელი იყო", "literal": "Tension unbearable was", "context": "Describing intense moment", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-035", "english": "Everything happened in slow motion", "georgian": "ყველაფერი ნელა ხდებოდა, თითქოს ფილმში", "literal": "Everything slowly was happening, as if in movie", "context": "Describing surreal moment", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-036", "english": "And that's how I ended up here", "georgian": "და ასე აღმოვჩნდი აქ", "literal": "And so I found myself here", "context": "Concluding a story about how you got somewhere", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-037", "english": "I'll never forget that day", "georgian": "იმ დღეს არასდროს დავივიწყებ", "literal": "That day never I'll forget", "context": "Memorable event", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-038", "english": "Looking back, I have no regrets", "georgian": "უკან რომ ვიხედები, არაფერზე ვნანობ", "literal": "Back when I look, nothing I regret", "context": "Reflecting on past decisions", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-039", "english": "And the moral of the story is", "georgian": "და ამ ამბის მორალია", "literal": "And this story's moral is", "context": "Stating the lesson learned", "level": "C1", "category": "storytelling-narrative"},
  {"id": "storytelling-040", "english": "That's my story and I'm sticking to it", "georgian": "ეს ჩემი ვერსიაა და ამაზე ვრჩები", "literal": "This my version is and on this I stay", "context": "Humorous way to end a story", "level": "C1", "category": "storytelling-narrative"}
]