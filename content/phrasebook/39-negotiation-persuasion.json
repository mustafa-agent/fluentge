[
  {"id": "negotiation-001", "english": "Let's find a middle ground", "georgian": "მოდით, შუალედური გადაწყვეტა ვიპოვოთ", "literal": "Let's middle solution find", "context": "Proposing compromise in negotiation", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-002", "english": "What would it take to make this work?", "georgian": "რა უნდა იმისთვის, რომ ეს იმუშაოს?", "literal": "What is needed for this to work?", "context": "Exploring possibilities in a deal", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-003", "english": "I see your point, but consider this", "georgian": "შენი აზრი მესმის, მაგრამ ესეც გაითვალისწინე", "literal": "Your thought I hear, but this also consider", "context": "Acknowledging before countering", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-004", "english": "That's my final offer", "georgian": "ეს ჩემი საბოლოო შეთავაზებაა", "literal": "This my final offer is", "context": "Setting a firm boundary in negotiation", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-005", "english": "Can you sweeten the deal?", "georgian": "შეგიძლია შეთავაზება უფრო მიმზიდველი გახადო?", "literal": "Can you offer more attractive make?", "context": "Asking for better terms", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-006", "english": "Let me play devil's advocate for a second", "georgian": "ნება მომეცით, ცოტა ეშმაკის ადვოკატი ვითამაშო", "literal": "Permission give me, a little devil's advocate I play", "context": "Presenting opposing view to test argument", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-007", "english": "You drive a hard bargain", "georgian": "შენთან მოლაპარაკება ძნელია", "literal": "With you negotiation hard is", "context": "Complimenting someone's negotiation skills", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-008", "english": "I'm willing to compromise on that", "georgian": "ამ საკითხში კომპრომისზე მზად ვარ", "literal": "On this issue compromise ready I am", "context": "Showing flexibility", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-009", "english": "Trust me, this is a win-win", "georgian": "მერწმუნე, ორივე მხარე მოგებული იქნება", "literal": "Trust me, both sides winning will be", "context": "Persuading both parties benefit", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-010", "english": "The ball is in your court", "georgian": "ბურთი შენს მოედანზეა", "literal": "Ball your court-on is", "context": "It's the other person's turn to decide", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-011", "english": "Let's not beat around the bush", "georgian": "პირდაპირ ვთქვათ", "literal": "Directly let's say", "context": "Getting straight to the point", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-012", "english": "What's your bottom line?", "georgian": "რა არის შენი მინიმალური მოთხოვნა?", "literal": "What is your minimum demand?", "context": "Asking for minimum acceptable terms", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-013", "english": "I'll need to think it over", "georgian": "მჭირდება ამაზე დაფიქრება", "literal": "I need on this to think", "context": "Buying time before deciding", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-014", "english": "Let's agree to disagree", "georgian": "დავთანხმდეთ, რომ არ ვეთანხმებით", "literal": "Let's agree that we don't agree", "context": "Ending disagreement peacefully", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-015", "english": "I can see where you're coming from", "georgian": "მესმის, საიდან მოდიხარ", "literal": "I understand, from where you come", "context": "Showing empathy before disagreeing", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-016", "english": "Here's what I propose", "georgian": "აი, რას გთავაზობთ", "literal": "Here, what I offer you", "context": "Presenting a formal proposal", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-017", "english": "Take it or leave it", "georgian": "ან მიიღე, ან არა", "literal": "Either accept, or not", "context": "Ultimatum - no more negotiation", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-018", "english": "Let's sleep on it", "georgian": "მოდი, ხვალ გადავწყვიტოთ", "literal": "Come, tomorrow let's decide", "context": "Suggesting to decide tomorrow", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-019", "english": "You won't find a better deal anywhere", "georgian": "უკეთეს შეთავაზებას ვერსად იპოვი", "literal": "Better offer nowhere you'll find", "context": "Creating urgency in a deal", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-020", "english": "I'm not budging on this one", "georgian": "ამ საკითხში არ დავთმობ", "literal": "On this issue not I'll give in", "context": "Standing firm on a point", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-021", "english": "Let me put it this way", "georgian": "ასე ვთქვათ", "literal": "So let's say", "context": "Reframing an argument", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-022", "english": "What if we split the difference?", "georgian": "რა იქნება, სხვაობა შუაზე გავიყოთ?", "literal": "What if difference in half we divide?", "context": "Classic compromise technique", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-023", "english": "I need more time to consider", "georgian": "მეტი დრო მჭირდება დასაფიქრებლად", "literal": "More time I need for thinking", "context": "Requesting more time", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-024", "english": "With all due respect, I disagree", "georgian": "ყოველგვარი პატივისცემით, არ ვეთანხმები", "literal": "With all respect, not I agree", "context": "Politely disagreeing", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-025", "english": "Let's look at the bigger picture", "georgian": "მოდით, სრულ სურათს შევხედოთ", "literal": "Let's full picture look at", "context": "Broadening perspective in discussion", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-026", "english": "I'll throw in a bonus", "georgian": "ბონუსს დავამატებ", "literal": "Bonus I'll add", "context": "Sweetening a deal", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-027", "english": "We're going around in circles", "georgian": "წრეზე ვტრიალებთ", "literal": "On circle we spin", "context": "Frustrated that discussion isn't progressing", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-028", "english": "Let me be straight with you", "georgian": "პირდაპირ გეტყვი", "literal": "Directly I'll tell you", "context": "Being honest and direct", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-029", "english": "Think about what you're giving up", "georgian": "იფიქრე, რას კარგავ", "literal": "Think, what you lose", "context": "Creating fear of missing out", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-030", "english": "I need a guarantee", "georgian": "გარანტია მჭირდება", "literal": "Guarantee I need", "context": "Requesting assurance", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-031", "english": "That's a deal breaker for me", "georgian": "ეს ჩემთვის მიუღებელია", "literal": "This for me unacceptable is", "context": "Non-negotiable point", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-032", "english": "I'm open to suggestions", "georgian": "წინადადებებისთვის ღია ვარ", "literal": "For suggestions open I am", "context": "Inviting proposals", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-033", "english": "Don't miss this opportunity", "georgian": "ამ შესაძლებლობას ნუ გაუშვებ", "literal": "This opportunity don't let go", "context": "Creating urgency", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-034", "english": "We can work something out", "georgian": "რამეს მოვიფიქრებთ", "literal": "Something we'll think of", "context": "Expressing willingness to negotiate", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-035", "english": "I'll meet you halfway", "georgian": "შუაგზაზე შეგხვდები", "literal": "On middle road I'll meet you", "context": "Offering compromise", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-036", "english": "This offer expires tomorrow", "georgian": "ეს შეთავაზება ხვალ იწურება", "literal": "This offer tomorrow expires", "context": "Creating time pressure", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-037", "english": "What do I get in return?", "georgian": "სანაცვლოდ რა მივიღებ?", "literal": "In return what I'll get?", "context": "Asking for reciprocity", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-038", "english": "That's a fair point", "georgian": "სამართლიანი შენიშვნაა", "literal": "Fair remark is", "context": "Conceding a valid argument", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-039", "english": "I'd rather not go below that price", "georgian": "ამ ფასზე ქვემოთ ჩასვლა არ მინდა", "literal": "This price below going I don't want", "context": "Protecting minimum price", "level": "C1", "category": "negotiation-persuasion"},
  {"id": "negotiation-040", "english": "Do we have a deal?", "georgian": "შეთანხმდით?", "literal": "Did you agree?", "context": "Closing a negotiation", "level": "C1", "category": "negotiation-persuasion"}
]